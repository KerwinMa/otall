<form class="form-horizontal" method="post">
  <fieldset>
    <legend>建立新项目</legend>
    <div class="control-group">
      <label class="control-label" for="username">项目名称</label>
      <div class="controls">
        <input type="text" class="input-xlarge" id="prjname" name="prjname">
      </div>
    </div>
    <div class="control-group">
      <label class="control-label" for="username">Apple AppID(可多个用逗号隔开)</label>
      <div class="controls">
        <input type="text" class="input-xlarge" id="appleAppIDs" name="appleAppIDs">
      </div>
    </div>
    <div class="form-actions">
      <button type="submit" class="btn btn-primary">创建</button>
    </div>
  </fieldset>
</form>