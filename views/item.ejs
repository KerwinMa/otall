<li data-theme="c">
    <a href="<%= item.url %>" data-ajax="false" data-transition="slide">
		<div class=fl_time>
			<% var fname = item.getFilename() %>
			<% if(item.group!=''){ %>
	        [<%= item.group %>] 
			<% }%>
			<%= item.time %>
		</div>
		<div class=fl_name><%= fname %></div>
		<div class=fl_comment><%= item.comment %></div>
    </a>
</li>