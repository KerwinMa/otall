<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
  <head>
    <title><%= title %> - OTA Server</title>
    <style type="text/css">
      body {
        padding-top: 60px;
        padding-bottom: 40px;
      }
    </style>
  </head>
  <body>
      <div id="container" class="container">
        <% if (success) { %>
          <div class="alert alert-success">
            <%= success %>
          </div>
        <% } %>
        <% if (error) { %>
          <div class="alert alert-error">
            <%= error %>
          </div>
        <% } %>
        <%- body %>
        <hr />
        <footer>
          <p><a href="javascript:;">DreamInGame</a> 2013</p>
        </footer>
      </div>
  </body>
</html>